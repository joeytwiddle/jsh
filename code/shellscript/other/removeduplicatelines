# LINESIN=`cat $* | awk ' { printf("\""$_"\"\n"); } '`
LINESIN=`cat "$@"`
echo "$LINESIN"
# exit 1
LINES=""
echo "$LINESIN" | while read LINE; do
  echo "$LINES" | while read X; do
    # echo 'Comparing "'$LINE'" "'$X'"'
    GOT=""
    if [ "$LINE" = "$X" ]; then
      GOT="true"
      # echo "************* ALREADY GOT"
    fi
  done
  if [ ! "$GOT" = "true" ]; then
    LINES="$LINES$LINE
"
    # echo "Adding $LINE"
  else
    # echo "Already got $LINE"
  fi
done
echo "$LINES"
