echo Filfirin
#umount /mnt/filfirin/
automount
cp /etc/fstab.exec /etc/fstab
mount /mnt/filfirin/

echo Stig
umount /mnt/stig/
mount /mnt/stig/

echo CDrom
# Disabled so as not to interrupt cd playing
mount /mnt/cdrom

echo Floppy
mount /mnt/floppy

