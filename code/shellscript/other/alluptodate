# Records the time at which all files correspond
# with those at HQ.
# Later, we can use findchanges to see which files
# need to be taken back to HQ.

date > $JPATH/uptodate.txt
echo "All files up to date" >> $JPATH/uptodate.txt
echo "Do not edit this file!" >> $JPATH/uptodate.txt

echo "Recorded files up to date in $JPATH/uptodate.txt"