export SHELL ## dircolors wants this
if test -f $HOME/.lscols
then . $HOME/.lscols
elif test -f "$HOME/.dircolors"
then eval `dircolors $HOME/.dircolors`
elif test -f "$JPATH/code/home/.dircolors"
then eval `dircolors $JPATH/code/home/.dircolors`
else eval `dircolors -p | dircolors`
fi
export LS_COLORS
